[saved-setup]
[labels]
title=Selected Common Polar Graphs

[grid]
left=-8
right=8
top=auto
bottom=auto

[options]
paper=polar
axes=1040
labels=on

[equations]
r^2 = 32 cos (2t) {-1p, 0} ' lemniscate (graphed over -pi to 0)
r=3/t ' hyperbolic spiral
r=2t ' spiral of Archimedes
r=4[1+cos(t)]  ' cardioid
r=2[1+2cos(t)]  ' limacon
r = 4sin(2t) ' four-leaved rose
